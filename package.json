{
  "name": "slog-analyze",
  "private": true,
  "version": "0.0.1",
  "keywords": [
    "cli",
    "log",
    "analyzer"
  ],
  "author": "steelWinds",
  "license": "MPL-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/steelWinds/slog-analyze"
  },
  "type": "module",
  "engines": {
    "node": ">=18.0.0"
  },
  "os": [
    "darwin",
    "linux",
    "win32"
  ],
  "scripts": {
    "build": "tsdown --env-file .env.production --minify --log-level error",
    "build:dev": "tsdown --env-file .env.development --sourcemap",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "lint": "oxlint --type-aware",
    "lint:fix": "oxlint --fix",
    "lint-staged": "lint-staged",
    "prepare": "husky"
  },
  "devDependencies": {
    "@faker-js/faker": "^10.2.0",
    "@types/node": "^25.0.9",
    "@vitest/coverage-v8": "^4.0.17",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "memfs": "^4.56.9",
    "oxlint": "^1.42.0",
    "oxlint-tsgolint": "^0.11.2",
    "tsdown": "0.20.1",
    "typescript": "~5.9.3",
    "vitest": "^4.0.17"
  },
  "dependencies": {
    "@commander-js/extra-typings": "^14.0.0",
    "chalk": "^5.6.2",
    "commander": "^14.0.2",
    "consola": "^3.4.2",
    "ora": "^9.0.0"
  },
  "overrides": {
    "tsdown": "0.20.1"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,mjs,cjs}": [
      "pnpm run lint",
      "pnpm run test:run"
    ]
  }
}
